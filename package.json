{
  "name": "radarsbutt-twitch",
  "version": "1.1.14",
  "description": "Twitch bot originally used for https://twitch.tv/radarcz",
  "repository": {
    "type": "git",
    "url": "https://github.com/RadarCZ/radarsbutt-twitch.git"
  },
  "scripts": {
    "build": "node ./util/build.js",
    "lint": "eslint -c .eslintrc.js --ext .ts ./src",
    "start": "node -r module-alias/register ./dist",
    "start:dev": "nodemon --config nodemon.json",
    "test": "nodemon --config nodemon.test.json",
    "docker:local": "docker-compose up -d --build && docker-compose logs -f"
  },
  "license": "AGPL-3.0-or-later",
  "_moduleAliases": {
    "@entities": "dist/entities",
    "@shared": "dist/shared",
    "@server": "dist/Server",
    "@twitch": "dist/twitch"
  },
  "dependencies": {
