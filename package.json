    "command-line-args": "^5.1.1",
    "cookie-parser": "^1.4.5",
    "cron": "^1.8.2",
    "css-loader": "^3.5.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "hbs": "^4.1.1",
    "helmet": "^3.22.0",
    "http-status-codes": "^1.4.0",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "tmi.js": "^1.5.0",
    "winston": "^3.2.1",
    "ws": "^7.2.5"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
